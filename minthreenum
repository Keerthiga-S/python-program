def min(a,b,c):

    if (a <= b and a <= c):
        smallest = a

    elif (b <= a and b <= c):
        smallest = b

    else:
        smallest = c
    
    return smallest

a=int(input("Enter the a value:"))
b=int(input("Enter the b value:"))
c=int(input("Enter the c value:"))

print(min(a,b,c))
     
