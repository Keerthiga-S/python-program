'''
n=int(input())
count=0
for i in range(0,n):
    for j in range(0,i+1):
        count=count + 1
        print(count," ",end=" ")#like right angle triangle
    print(" ")
'''
n=int(input())
for i in range(0,n):
    for k in range(0,n-i-1):
        print(" ",end=" ")
    for j in range(0,i+1):
        print(i+1," ",end=" ")#like pyramid triangle
    print(" ")