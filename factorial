'''
num= int(input("Enter the number:"))
factorial = 1
if num < 0:
    print("Factorial is not exist for the negative number")

elif num == 0:
    print("Factorial of 0 is 1")

else:
    for i in range(1,num + 1):
        factorial=factorial*i
    print(factorial)
    '''

def factorial(num):
    if num == 0:
        return 1
    
    else:
        return num * factorial(num-1)

num=int(input("Enter the number:"))
result =factorial(num)
print(result)